<?xml version="1.0" encoding="utf-8"?>
<!-- This file, the project manifest, provides the overall configuration for your PreTeXt project. To edit the content of your document, open `source/main.ptx`. See https://pretextbook.org/doc/guide/html/processing-CLI.html#cli-project-manifest. -->
<project ptx-version="2">
  <targets>
    <target name="web" format="html" publication="../publication/publication.ptx"  >
      <stringparams html.css.extra="external/custom-styles.css"  debug.project.number="the-way-it-should-be"/> 
    </target>
    <target name="print" format="pdf"  latex-engine="xelatex" publication="../publication/publicationprint.ptx" >
    <stringparams debug.project.number="the-way-it-should-be" index-maker="pretext" latex.preamble.late="\setlength\extrarowheight{5pt} &#xa; \newcommand{\sectionbreak}{\ifnum\value{section}>1\clearpage\thispagestyle{plain}\fi\phantomsection}"/> 
    </target>
    <target name="tex" format="latex"  latex-engine="xelatex" publication="../publication/publicationprint.ptx" >
    <stringparams debug.project.number="the-way-it-should-be" index-maker="pretext"  latex.preamble.late="\setlength\extrarowheight{5pt} &#xa; \newcommand{\sectionbreak}{\ifnum\value{section}>1\clearpage\thispagestyle{plain}\fi\phantomsection}"/> 
    </target>
  </targets>
</project>